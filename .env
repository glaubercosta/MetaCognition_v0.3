APP_ENV=dev
LOG_LEVEL=INFO
DEFAULT_ENGINE=langchain
DATABASE_URL=sqlite:///data/app.db

# Import / validation limits
IMPORT_MAX_FILE_MB=2
IMPORT_MAX_ITEMS=25
PROMPT_MAX_BYTES=8192

# LangChain engine configuration
LANGCHAIN_PROVIDER=stub
LANGCHAIN_MODEL=
LANGCHAIN_API_KEY=
LANGCHAIN_BASE_URL=
LANGCHAIN_TEMPERATURE=0

# Legacy CrewAI settings (opcionais)
CREWAI_MODE=stub
CREWAI_HTTP_MODE=dry-run
CREWAI_API_KEY=
CREWAI_BASE_URL=https://api.crewai.example
CREWAI_RUN_PATH=/v1/run
CREWAI_MODEL=crewai-large
CREWAI_TIMEOUT_SEC=30
CREWAI_MAX_RETRIES=2
CREWAI_BACKOFF_SEC=0.5
